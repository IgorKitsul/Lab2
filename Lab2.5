# Lab2
using System;

class Program
{
    static void Main(string[] args)
    {
        const int n = 10; 
        const int m = 5; 

        int[,] A = new int[n, m];

        Random rnd = new Random();

        
        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < m; j++)
            {
                A[i, j] = rnd.Next(-100, 101);
            }
        }

        Console.WriteLine("Матриця:");

      
        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < m; j++)
            {
                Console.Write(A[i, j] + " ");
            }
            Console.WriteLine();
        }

        Console.WriteLine("Рядки з нульовим елементом:");

        
        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < m; j++)
            {
                if (A[i, j] == 0)
                {
                    Console.WriteLine(i);
                    break;
                }
            }
        }
    }
}
